	"tokenColors": [
		{
			"name": "Operators",
			"scope": "punctuation.definition.bracket.go, punctuation.definition.dot, punctuation.definition.comma, keyword.operator, punctuation.definition.colon",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Quotes",
			"scope": "punctuation.definition.string",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#128a00"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ff79d2"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#fada66"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#ffdf00"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#00ffdd"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"foreground": "#ff0000",
				"fontStyle": " bold"
			}
		},
		{
			"name": "String",
			"scope": "string, meta.verbatim",
			"settings": {
				"foreground": "#1ed500"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"foreground": "#fac501"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#2380db",
				"fontStyle": " italic"
			}
		},
		{
			"name": "Invalid – Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#0FF"
			}
		},
		{
			"name": "Invalid – Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#5d0600",
				"foreground": "#ffffff",
				"fontStyle": " underline"
			}
		},
		{
			"name": "Superclass",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic bold",
				"foreground": "#FF6400"
			}
		},
		{
			"name": "String interpolation",
			"scope": "string constant.other.placeholder",
			"settings": {
				"foreground": "#C9F"
			}
		},
		{
			"name": "meta.tag",
			"scope": "meta.tag, meta.tag entity",
			"settings": {
				"foreground": "#a46ec2",
				"fontStyle": " italic "
			}
		},
		{
			"name": "entity.name.section",
			"scope": "entity.name.section",
			"settings": {
				"fontStyle": "",
				"foreground": "#FF0"
			}
		},
		{
			"name": "Comment.documentation",
			"scope": "Comment.documentation",
			"settings": {
				"foreground": "#00c776"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796e6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#cd9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#b267e6"
			}
		}
	]
}